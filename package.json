{
  "name": "bigsandbox",
  "version": "0.0.1",
  "author": "Matthew Volk",
  "type": "module",
  "exports": {
    "./*": {
      "import": "./dist/*.js",
      "require": "./dist/*.cjs"
    },
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "module": "./dist/index.js",
  "main": "./dist/index.cjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "packageManager": "pnpm@10.0.0",
  "scripts": {
    "dev": "vitest",
    "fmt": "prettier . --check",
    "lint": "eslint --max-warnings 0",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "build": "tsup",
    "attw": "attw --pack .",
    "agadoo": "agadoo"
  },
  "devDependencies": {
    "@arethetypeswrong/cli": "^0.17.3",
    "@eslint/js": "^9.17.0",
    "agadoo": "^3.0.0",
    "eslint": "^9.17.0",
    "eslint-plugin-import": "^2.31.0",
    "globals": "^15.14.0",
    "prettier": "3.5.1",
    "tsup": "^8.3.6",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.19.1",
    "vitest": "^2.1.8"
  },
  "dependencies": {
    "@types/node": "^22.13.4"
  }
}
